div#params_plugin433
	div(class='form-group')
		label(class='col-lg-2 control-label' for='setting_plugin433_grpId') Group ID
		div(class='col-lg-10')
			select(class='form-control' id='setting_plugin433_grpId' size='1')
				option(value='1') 1
				option(value='2') 2
				option(value='3') 3
				option(value='4') 4
	div(class='form-group')
		label(class='col-lg-2 control-label' for='setting_plugin433_deviceId') Device ID
		div(class='col-lg-10')
			select(class='form-control' id='setting_plugin433_deviceId' size='1')
				option(value='1') 1
				option(value='2') 2
				option(value='3') 3
				option(value='4') 4
	div(class='form-group')
		label(class='col-lg-2 control-label' for='setting_plugin433_value') Value
		div(class='col-lg-10')
			select(class='form-control' id='setting_plugin433_value' size='1')
				option(value='0') 0
				option(value='1') 1
script(src="/javascripts/plugin433.js")
